{"ast":null,"code":"export default {\n  data() {\n    return {\n      itemsPerPageArray: [4, 8, 12],\n      search: '',\n      filter: {},\n      sortDesc: false,\n      page: 1,\n      itemsPerPage: 4,\n      sortBy: 'name',\n      keys: ['Name', 'Calories', 'Fat', 'Carbs', 'Protein', 'Sodium', 'Calcium', 'Iron'],\n      items: [{\n        id: 1,\n        createTime: 2022 - 10 - 10,\n        outTradeNo: 111111,\n        consignee: \"小红\",\n        totalAmount: 100,\n        orderStatusName: \"已完成\"\n      }, {\n        id: 2,\n        createTime: 2022 - 11 - 10,\n        outTradeNo: 111112\n      }]\n    };\n  },\n  computed: {\n    numberOfPages() {\n      return Math.ceil(this.items.length / this.itemsPerPage);\n    },\n    filteredKeys() {\n      return this.keys.filter(key => key !== `Name`);\n    }\n  },\n  methods: {\n    nextPage() {\n      if (this.page + 1 <= this.numberOfPages) this.page += 1;\n    },\n    formerPage() {\n      if (this.page - 1 >= 1) this.page -= 1;\n    },\n    updateItemsPerPage(number) {\n      this.itemsPerPage = number;\n    }\n  }\n};","map":{"version":3,"mappings":"AA0JA;EACAA;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC,OACA,QACA,YACA,OACA,SACA,WACA,UACA,WACA,OACA;MACAC,QACA;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA,GACA;QACAL;QACAC;QACAC;MACA;IAEA;EACA;EACAI;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["data","itemsPerPageArray","search","filter","sortDesc","page","itemsPerPage","sortBy","keys","items","id","createTime","outTradeNo","consignee","totalAmount","orderStatusName","computed","numberOfPages","filteredKeys","methods","nextPage","formerPage","updateItemsPerPage"],"sourceRoot":"src/components/customer","sources":["orderTry.vue"],"sourcesContent":["<template>\r\n  <v-container fluid>\r\n    <v-data-iterator\r\n        :items=\"items\"\r\n        :items-per-page.sync=\"itemsPerPage\"\r\n        :page=\"page\"\r\n        :search=\"search\"\r\n        :sort-by=\"sortBy.toLowerCase()\"\r\n        :sort-desc=\"sortDesc\"\r\n        hide-default-footer\r\n    >\r\n      <template v-slot:header>\r\n        <div class=\"title\">\r\n          <h3>我的订单</h3>\r\n        </div>\r\n        <v-toolbar\r\n            dark\r\n            color=\"blue darken-3\"\r\n            class=\"mb-1\"\r\n        >\r\n          <v-text-field\r\n              clearable\r\n              flat\r\n              solo-inverted\r\n              hide-details\r\n              label=\"Search\"\r\n          ></v-text-field>\r\n          <template v-if=\"$vuetify.breakpoint.mdAndUp\">\r\n            <v-spacer></v-spacer>\r\n            <v-select\r\n                v-model=\"sortBy\"\r\n                flat\r\n                solo-inverted\r\n                hide-details\r\n                :items=\"keys\"\r\n                prepend-inner-icon=\"search\"\r\n                label=\"Sort by\"\r\n            ></v-select>\r\n            <v-spacer></v-spacer>\r\n            <v-btn-toggle\r\n                v-model=\"sortDesc\"\r\n                mandatory\r\n            >\r\n              <v-btn\r\n                  large\r\n                  depressed\r\n                  color=\"blue\"\r\n                  :value=\"false\"\r\n              >\r\n                <v-icon>mdi-arrow-up</v-icon>\r\n              </v-btn>\r\n              <v-btn\r\n                  large\r\n                  depressed\r\n                  color=\"blue\"\r\n                  :value=\"true\"\r\n              >\r\n                <v-icon>mdi-arrow-down</v-icon>\r\n              </v-btn>\r\n            </v-btn-toggle>\r\n          </template>\r\n        </v-toolbar>\r\n      </template>\r\n\r\n      <template v-slot:default=\"props\">\r\n        <v-row>\r\n          <v-col\r\n              v-for=\"item in props.items\"\r\n              :key=\"item.name\"\r\n              cols=\"12\"\r\n              sm=\"6\"\r\n              md=\"4\"\r\n              lg=\"3\"\r\n          >\r\n            <v-card>\r\n              <v-card-title class=\"subheading font-weight-bold\">{{ item.name }}</v-card-title>\r\n\r\n              <v-divider></v-divider>\r\n\r\n              <v-list dense>\r\n                <v-list-item\r\n                    v-for=\"(key, index) in filteredKeys\"\r\n                    :key=\"index\"\r\n                >\r\n                  <v-list-item-content :class=\"{ 'blue--text': sortBy === key }\">{{ key }}:</v-list-item-content>\r\n                  <v-list-item-content class=\"align-end\" :class=\"{ 'blue--text': sortBy === key }\">{{ item[key.toLowerCase()] }}</v-list-item-content>\r\n                </v-list-item>\r\n              </v-list>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </template>\r\n\r\n      <template v-slot:footer>\r\n        <v-row class=\"mt-2\" align=\"center\" justify=\"center\">\r\n          <span class=\"grey--text\">Items per page</span>\r\n          <v-menu offset-y>\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n              <v-btn\r\n                  dark\r\n                  text\r\n                  color=\"primary\"\r\n                  class=\"ml-2\"\r\n                  v-bind=\"attrs\"\r\n                  v-on=\"on\"\r\n              >\r\n                {{ itemsPerPage }}\r\n                <v-icon>mdi-chevron-down</v-icon>\r\n              </v-btn>\r\n            </template>\r\n            <v-list>\r\n              <v-list-item\r\n                  v-for=\"(number, index) in itemsPerPageArray\"\r\n                  :key=\"index\"\r\n                  @click=\"updateItemsPerPage(number)\"\r\n              >\r\n                <v-list-item-title>{{ number }}</v-list-item-title>\r\n              </v-list-item>\r\n            </v-list>\r\n          </v-menu>\r\n\r\n          <v-spacer></v-spacer>\r\n\r\n          <span\r\n              class=\"mr-4\r\n            grey--text\"\r\n          >\r\n            Page {{ page }} of {{ numberOfPages }}\r\n          </span>\r\n          <v-btn\r\n              fab\r\n              dark\r\n              color=\"blue darken-3\"\r\n              class=\"mr-1\"\r\n              @click=\"formerPage\"\r\n          >\r\n            <v-icon>mdi-chevron-left</v-icon>\r\n          </v-btn>\r\n          <v-btn\r\n              fab\r\n              dark\r\n              color=\"blue darken-3\"\r\n              class=\"ml-1\"\r\n              @click=\"nextPage\"\r\n          >\r\n            <v-icon>mdi-chevron-right</v-icon>\r\n          </v-btn>\r\n        </v-row>\r\n      </template>\r\n    </v-data-iterator>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      itemsPerPageArray: [4, 8, 12],\r\n      search: '',\r\n      filter: {},\r\n      sortDesc: false,\r\n      page: 1,\r\n      itemsPerPage: 4,\r\n      sortBy: 'name',\r\n      keys: [\r\n        'Name',\r\n        'Calories',\r\n        'Fat',\r\n        'Carbs',\r\n        'Protein',\r\n        'Sodium',\r\n        'Calcium',\r\n        'Iron',\r\n      ],\r\n      items:[\r\n        {\r\n          id: 1,\r\n          createTime: 2022-10-10,\r\n          outTradeNo: 111111,\r\n          consignee:\"小红\",\r\n          totalAmount:100,\r\n          orderStatusName:\"已完成\",\r\n        },\r\n        {\r\n          id: 2,\r\n          createTime: 2022-11-10,\r\n          outTradeNo: 111112,\r\n        }\r\n      ],\r\n    }\r\n  },\r\n  computed: {\r\n    numberOfPages () {\r\n      return Math.ceil(this.items.length / this.itemsPerPage)\r\n    },\r\n    filteredKeys () {\r\n      return this.keys.filter(key => key !== `Name`)\r\n    },\r\n  },\r\n  methods: {\r\n    nextPage () {\r\n      if (this.page + 1 <= this.numberOfPages) this.page += 1\r\n    },\r\n    formerPage () {\r\n      if (this.page - 1 >= 1) this.page -= 1\r\n    },\r\n    updateItemsPerPage (number) {\r\n      this.itemsPerPage = number\r\n    },\r\n  },\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}